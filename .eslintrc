{
  "rules": {
    "semi": [
      2,
      "always"
    ],
    "no-console": 0,
    "no-underscore-dangle": 0,
    "eqeqeq": [
      "error",
      "always",
      {
        "null": "ignore"
      }
    ],
    "arrow-parens": [
      "error",
      "always"
    ],
    "import/no-dynamic-require": "off",
    "no-param-reassign": "off",
    "strict": [
      "error",
      "never"
    ]
  },
  "extends": "eslint-config-egg"
}
